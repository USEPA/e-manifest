{
  "name": "emanifest",
  "version": "0.3.2",
  "description": "API client library, written in TypeScript, for using the EPA e-Manifest/RCRAInfo web services.",
  "type": "module",
  "main": "./dist/emanifest-lib.umd.cjs",
  "module": "./dist/emanifest-lib.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/emanifest-lib.js",
      "require": "./dist/emanifest-lib.umd.cjs"
    }
  },
  "scripts": {
    "build": "tsc && vite build",
    "test": "vitest --run",
    "test:watch": "vitest --watch"
  },
  "dependencies": {
    "axios": "^1.7.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@types/eslint__js": "^8.42.3",
    "@types/jest": "^29.5.12",
    "@types/node": "^22.5.1",
    "eslint": "^9.9.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "jest": "^29.7.0",
    "msw": "^2.3.5",
    "prettier": "3.3.3",
    "ts-jest": "^29.2.5",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.4.2",
    "vite-plugin-dts": "^4.0.3",
    "vitest": "^2.0.5"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "TypeScript",
    "API",
    "e-Manifest",
    "hazardous",
    "waste",
    "EPA"
  ],
  "author": "David Graham",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/USEPA/emanifest/issues"
  },
  "homepage": "https://github.com/USEPA/emainfest"
}
